<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.willaboy.whatpicture">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity_Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity_AddPhoto"></activity>
        <activity android:name=".Activity_Main"></activity>
        <activity android:name=".Activity_ViewPhotos"></activity>
        <activity android:name=".Activity_PuzzleGame"></activity>
        <activity android:name=".Activity_ChoosePuzzleMenu"></activity>
        <activity android:name=".Activity_ViewPhotosCategory"></activity>
        <activity android:name=".Activity_Register"></activity>
    </application>

</manifest>